        <jsScript_script>&#47;&#47;Script here

var name = company_name;

if (name!=null)
{
  var CODE_FULL = CODE_FULL_CODES.split(&quot;, &quot;);
  for each (var codeFull in CODE_FULL)
  {
     var pieces = codeFull.split(&quot;;&quot;);
     var code = pieces[0];
     var full = pieces[1];

     name = replace(name, code, full);
  } 
}
</jsScript_script>
