   <operator>AND</operator>
   <leftvalue>e</leftvalue>
   <function>&lt;</function>
   <rightvalue/>
   <value><name>constant</name><type>Number</type><text>500.0</text><length>-1</length><precision>-1</precision><isnull>N</isnull></value>   </condition>
  </conditions>
 </condition>
    </compare>
     <cluster_schema/>
    <GUI>
      <xloc>363</xloc>
      <yloc>171</yloc>
      <draw>Y</draw>
      </GUI>
