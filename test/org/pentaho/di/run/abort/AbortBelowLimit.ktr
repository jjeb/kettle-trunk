<?xml version="1.0" encoding="UTF-8"?>
<transformation>
  <info>
    <name>Abort Below Limit</name>
    <description>Tests the functionality and performance of the &quot;Abort&quot; step.</description>
    <extended_description>This transform doesn&apos;t abort as the limit is not reached.</extended_description>
    <trans_version/>
    <trans_status>0</trans_status>
    <directory>&#47;</directory>
    <log>
      <read/>
      <write/>
      <input/>
      <output/>
      <update/>
      <rejected/>
      <connection/>
      <table/>
      <use_batchid>Y</use_batchid>
      <use_logfield>N</use_logfield>
    </log>
    <maxdate>
      <connection/>
      <table/>
      <field/>
      <offset>0.0</offset>
      <maxdiff>0.0</maxdiff>
    </maxdate>
    <size_rowset>7500</size_rowset>
    <sleep_time_empty>1</sleep_time_empty>
    <sleep_time_full>1</sleep_time_full>
    <unique_connections>N</unique_connections>
    <feedback_shown>Y</feedback_shown>
    <feedback_size>100000</feedback_size>
    <using_thread_priorities>N</using_thread_priorities>
    <shared_objects_file/>
    <dependencies>
    </dependencies>
    <partitionschemas>
        <partitionschema>
          <name>MySQL Partitioning schema</name>
          <partition>            <id>P1</id>
          </partition>          <partition>            <id>P2</id>
          </partition>          <partition>            <id>P3</id>
          </partition>          <partition>            <id>P4</id>
          </partition>        </partitionschema>
    </partitionschemas>
    <slaveservers>
    </slaveservers>
    <clusterschemas>
    </clusterschemas>
  <modified_user>-</modified_user>
  <modified_date>2007&#47;05&#47;09 17:55:58.156</modified_date>
  </info>
  <notepads>
  </notepads>
  <order>
  <hop> <from>Generate Rows</from><to>Abort</to><enabled>Y</enabled> </hop>  </order>
  <step>
    <name>Abort</name>
    <type>Abort</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <field_name/>
           <schema_name/>
           </partitioning>
      <row_threshold>10</row_threshold>
      <message>Abort message</message>
      <always_log_rows>Y</always_log_rows>
     <cluster_schema/>
    <GUI>
      <xloc>281</xloc>
      <yloc>152</yloc>
      <draw>Y</draw>
      </GUI>
    </step>

  <step>
    <name>Generate Rows</name>
    <type>RowGenerator</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <field_name/>
           <schema_name/>
           </partitioning>
    <fields>
    </fields>
    <limit>10</limit>
     <cluster_schema/>
    <GUI>
      <xloc>150</xloc>
      <yloc>152</yloc>
      <draw>Y</draw>
      </GUI>
    </step>

  <step_error_handling>
  </step_error_handling>
</transformation>
